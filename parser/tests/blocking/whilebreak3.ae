#include "aesop.h"
#include "parser/tests/blocking/btest.hae"

static __blocking void run_whilebreak(void)
{
	int a = 0;
	int ret;
	
	while(1)
	{
		ret = btest1(&a);
		assert(ret == 0);
		ret = btest2(&a);
		assert(ret == 0);
		if(a > 1000)
		{
		    break;
		}
		ret = btest3(&a);
		assert(ret == 0);
	}

	ret = btest1(&a);
	assert(ret == 0);
	ret = btest2(&a);
	assert(ret == 0);
	ret = btest3(&a);
	assert(ret == 0);
}
	
__blocking int aesop_main(int argc, char **argv)
{
    run_whilebreak();
    return 0;
}
btest_main_set(aesop_main, "btest");
