#include "aesop.h"
#include "parser/tests/blocking/btest.hae"
#include <assert.h>

int val = 0;

static __blocking void run_ifnoelse2(void)
{
	int a = 0;
	int ret;
	
	if(val == 0)
	{
		ret = btest1(&a);
		assert(ret == 0);
		assert(a == 1);
	}

	a += 10;
        ret = btest3(&a);
        assert(ret == 0);
	if(val == 0) assert(a == 14);
	else assert(a == 13);
}

__blocking int aesop_main(int argc, char **argv)
{
    run_ifnoelse2();
    return 0;
}
aesop_main_set(aesop_main, "btest");

