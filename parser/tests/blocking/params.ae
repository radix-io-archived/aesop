#include <aesop/aesop.h>
#include "parser/tests/blocking/btest.hae"

static __blocking void run_params(int a, const char *b, const char *c)
{
	int ret;
	ret = btest1(&a);
	printf("%s\n", b);
	assert(ret == 0);
	assert(a == 11);
	ret = btest2(&a);
	printf("%s\n", c);
	assert(ret == 0);
	assert(a == 13);
	ret = btest3(&a);
	printf("%d\n", a);
	assert(ret == 0);
	assert(a == 16);
}

__blocking int aesop_main(int argc, char **argv)
{
    btest_init();
    run_params(10, "FOO", "BAR");
    btest_finalize();
    return 0;
}
aesop_main_set(aesop_main);

