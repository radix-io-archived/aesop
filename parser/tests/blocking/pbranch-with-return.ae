#include <assert.h>
#include <aesop/aesop.h>
#include "parser/tests/blocking/btest.hae"

static __blocking int pbranch_with_return(void)
{

    pwait
    {
        pprivate int a = 0;

        pbranch
        {
            btest1(&a);
            return a;
        }

        pbranch
        {
            btest2(&a);
        }
    }

    return 0;
}

__blocking int aesop_main(int argc, char **argv)
{
    btest_init();
    pbranch_with_return();
    btest_finalize();
    return 0;
}
aesop_main_set(aesop_main);


