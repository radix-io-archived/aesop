#include <assert.h>
#include "src/aesop/aesop.h"
#include "src/aesop/parser/tests/blocking/btest.hae"

static __blocking int pbranch_with_return(void)
{

    pwait
    {
        pprivate int a = 0;

        pbranch
        {
            btest1(&a);
            return a;
        }

        pbranch
        {
            btest2(&a);
        }
    }

    return 0;
}

__blocking int aesop_main(int argc, char **argv)
{
    pbranch_with_return();
    return 0;
}
btest_main_set(aesop_main, "btest");

