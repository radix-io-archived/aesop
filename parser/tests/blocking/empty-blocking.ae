#include <aesop/aesop.h>

__blocking void noop (void)
{
   return;
}


__blocking void t1 (void)
{
   noop ();
}

__blocking void t2 (void)
{
   pwait
   {
      pbranch {
         noop ();
      }
   }
}

__blocking int aesop_main(int argc, char **argv)
{
   t1 ();
   t2 ();
   return 0;
}

aesop_main_set(aesop_main);


