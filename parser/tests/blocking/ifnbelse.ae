#include "src/aesop/aesop.h"
#include "src/aesop/parser/tests/blocking/btest.hae"
#include <assert.h>

int val = 0;

static __blocking void run_ifnbelse(void)
{
	int a = 0;
	int ret;
	
	if(val == 0)
	{
		ret = btest1(&a);
		assert(ret == 0);
		assert(a == 1);
	}
	else
	{
		printf("ELSE\n");
	}
        ret = btest3(&a);
        assert(ret == 0);
	if(val == 0) assert(a == 4);
	else assert(a == 3);
}

__blocking int aesop_main(int argc, char **argv)
{
    run_ifnbelse();
    return 0;
}
btest_main_set(aesop_main, "btest");

