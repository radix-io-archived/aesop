#include <unistd.h>
#include "aesop.h"
#include "parser/tests/blocking/btest.hae"
#include <assert.h>
#include "aesop-support.hae"

static __blocking void run_test(void)
{
        int ret1;
        int ret2;
        int a = 0;

	pwait
	{
	    pbranch
	    {
                sleep(5);
	    }

	    pbranch
	    {
                /* cancel the other pbranch */
                printf("pbranch2: calling aesop_cancel_branches_wait().\n");
                aesop_cancel_branches_wait();
                printf("pbranch2: done.\n");
	    }
	}

	printf("pwait done\n");

    return;
}

__blocking int aesop_main(int argc, char **argv)
{
    run_test();
    return 0;
}
aesop_main_set(aesop_main, "btest", "timer");

