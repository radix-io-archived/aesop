#include <assert.h>
#include <aesop/aesop.h>
#include "parser/tests/blocking/btest.hae"

static __blocking void run_basic(void)
{
	int ret;

	ret = btest_consec();
	assert(ret == 0);

	ret = btest_consec();
	assert(ret == 0);
}

__blocking int aesop_main(int argc, char **argv)
{
    btest_init();
    run_basic();
    btest_finalize();
    return 0;
}
aesop_main_set(aesop_main);


